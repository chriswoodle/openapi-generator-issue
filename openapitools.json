{
    "$schema": "@openapitools/openapi-generator-cli/config.schema.json",
    "spaces": 4,
    "generator-cli": {
        "version": "5.3.1",
        "generators": {
            "v1.0": {
                "generatorName": "typescript-axios",
                "output": "#{cwd}/client",
                "glob": "api-spec.json",
                "additionalProperties": {
                    "npmName": "client",
                    "supportsES6": "true",
                    "useSingleRequestParameter": true
                }
            }
        }
    }
}